syntax on
filetype on
set noswapfile
set number
" set cursorcolumn
" set colorcolumn=81
" highlight ColorColumn ctermbg=darkgrey
set expandtab
set tabstop=4
set softtabstop=4
set shiftwidth=4
set autoindent
set textwidth=80
set nobackup
set hlsearch
set showmatch
set clipboard^=unnamedplus,unnamed

